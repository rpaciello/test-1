			<foaf:primaryTopic>http://mapsrefdev.brgm.fr/wxs/1GE/EGDI_1M_INSPIRE_geolUnits?</foaf:primaryTopic>
		</foaf:page>
		<dct:format>
			<dct:MediaTypeOrExtent>wms</dct:MediaTypeOrExtent>
		</dct:format>
		<!--<dct:format>
			<dct:MediaTypeOrExtent>https://www.iana.org/assignments/media-types/image/png</dct:MediaTypeOrExtent>
		</dct:format>-->
		<dct:conformsTo>EPSG:4326</dct:conformsTo>
		<!--question : no sense to define it at GetCapabilities level -->
		<dct:identifier>http://mapsrefdev.brgm.fr/wxs/1GE/EGDI_1M_INSPIRE_geolUnits?</dct:identifier>
		<dct:created>2016-10-27T09:30:00</dct:created>
		<eposap:domain>Geology</eposap:domain>
		<eposap:subDomain>Geological Data</eposap:subDomain>
		<!--<dct:type>View Service</dct:type>-->
		<!--<dcat:keyword>http://inspire.ec.europa.eu/metadata-codelist/SpatialDataServiceCategory/infoMapAccessService</dcat:keyword>-->
		<!-- what type of keyword do you want ? kind of service type (see commented above) or domain (see below) ? -->
		<dcat:keyword>GeologicUnit, Geology, INSPIRE geology, GeoSciML</dcat:keyword>
		<eposap:operation>getmap</eposap:operation>
		<dct:hasVersion>1.3.0</dct:hasVersion>
		<!-- question : version is not of use for a GetCapabilities-->
		<!-- Several formats support is usual in OGC services -->
		<!-- Decision WP6/7 - WP15 20170724 webconf: the GUI will be able to interact generically with standards OGC services -> thus just the GetCapabilities is mapped. Few fields are mandatory  according to OGC 06-042 (WMS 1.3.0) -->
		<!-- This rationale implies that the ICS-C will learn dynamically the layer to interact with and the operations (GetMap, GetFeatureInfo)-->
